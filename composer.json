{
  "name": "dcmedia/blog-intro",
  "description": "Introduce your blog page to your audience using the built-in WordPress editor.",
  "type": "project",
  "keywords": [
    "wordpress",
    "wp",
    "plugin"
  ],
  "homepage": "http://example.com",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Dan Crews",
      "email": "dancrews@me.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "kint-php/kint": "^1.0",
    "mockery/mockery": "dev-master",
    "filp/whoops": "~2.0"
  },
  "autoload": {
    "classmap": [],
    "files": [
      "src/contents.php",
      "src/admin/enable-editor.php"
    ]
  },
  "autoload-dev": {
    "classmap": [],
    "files": [
      "src/support/exceptions.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "vendor-dir": "assets/vendor"
  },
  "minimum-stability": "dev"
}